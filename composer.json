{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.27",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/advagg": "^4.0",
        "drupal/components": "^1.1",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/entityqueue": "^1.0@beta",
        "drupal/minifyhtml": "^1.8",
        "drupal/minifyjs": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/webform": "^5.4",
        "drupal/webform_layout_container": "^1.1",
        "drupal/zen": "^7.0@alpha",
        "drush/drush": "^9.0.0",
        "jquery/inputmask": "*",
        "jquery/intl-tel-input": "*",
        "jquery/select2": "*",
        "php": ">=5.6",
        "signature_pad/signature_pad": "*",
        "twig/extensions": "^1.5",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/config_update_ui": "^1.6",
        "drupal/devel": "^2.1",
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/portfolio/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/profiles/custom/portfolio/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/custom/portfolio/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/portfolio/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "merge-plugin": {
            "include": [
                "composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/zen": {
                "Improves Eslint version and fix her errors": "https://www.drupal.org/files/issues/2934512.patch"
            }
        }
    }
}